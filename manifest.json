{
  "name": "Pacote de aprimoramento do Google Meet",
  "manifest_version": 2,
  "version": "1.0",
  "icons": { "48": "icons/border-48.png" },

  "content_scripts": [
    {
      "matches": ["*://meet.google.com/*"],
      "js": [
        "process/assets.js",
        "process/observer.js",
        "constructor/dashboard.js",
        "constructor/buttons.js",
        "background.js"
      ]
    }
  ],

  "web_accessible_resources": ["resources/dashboard.html"]
}

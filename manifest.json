{
  "name": "Pacote de aprimoramento do Google Meet",
  "manifest_version": 2,
  "version": "1.2",

  "content_scripts": [
    {
      "matches": ["*://meet.google.com/*"],
      "js": [
        "Assets/console.js",
        "Assets/constructor.js",
        "Background/initButton.js",
        "Background/index.js"
      ]
    }
  ],

  "web_accessible_resources": [
    "Structure/dashboard/index.html"
  ]
}
{
  "name": "Pacote de aprimoramento do Google Meet",
  "manifest_version": 2,
  "version": "1.0",
  "icons": { "48": "icons/border-48.png" },

  "content_scripts": [
    {
      "matches": ["*://meet.google.com/*"],
      "js": [
        "Background/assets.js",
        "Background/build.js",
        "Background/implant.js",
        "Background/trigger.js"
      ]
    }
  ],

  "web_accessible_resources": [
    "resources/dashboard.html",
    "resources/styles.css",
    "InfiltrateCode/Constructor/buttons.js",
    "InfiltrateCode/Process/observer.js",
    "InfiltrateCode/assets.js",
    "InfiltrateCode/trigger.js"
  ]
}
